{
  "name": "budget-app",
  "version": "1.0.0",
  "description": "Budget app developed for the Alkemy full stack challenge",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "dev": "cross-env NODE_ENV=development nodemon server",
    "build": "cd client/ && npm install && npm run build",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand --forceExit"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-torre/budget-app.git"
  },
  "keywords": [
    "node",
    "react",
    "express",
    "budget"
  ],
  "author": "Maximiliano Torre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-torre/budget-app/issues"
  },
  "homepage": "https://github.com/m-torre/budget-app#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "jest": "^28.0.2",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
